<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ren.yale.scrollviewslippingapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ListViewActivity">
        </activity>
        <activity android:name=".RecyclerActivity">
        </activity>
        <activity android:name=".ScrollViewActivity">
        </activity>
        <activity android:name=".WebViewActivity">
        </activity>
        <activity
            android:name=".BehaviorRecyclerActivity"
            android:label="@string/title_activity_behavior_recycler">
        </activity>
        <activity
            android:name=".BehaviorWebviewActivity"
            android:label="@string/title_activity_behavior_webview">
        </activity>
        <activity
            android:name=".BehaviorScrollViewActivity"
            android:label="@string/title_activity_behavior_scroll_view">
        </activity>
    </application>

</manifest>